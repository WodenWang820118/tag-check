{
  "name": "datalayer-checker",
  "version": "0.0.0",
  "main": "apps/frontend/main.js",
  "author": "Guan Xin Wang, Brandon Pardede",
  "description": "A tool to check the data layer of a website",
  "license": "MIT",
  "scripts": {
    "backend": "nx serve nest-datalayer-checker --watch",
    "mock-project-backend": "json-server db-projects.json --id projectSlug --watch --port 3000",
    "mock-recording-backend": "json-server db-recordings.json --id projectSlug --watch --port 3001",
    "mock-configurations-backend": "json-server db-configurations.json --id name --watch --port 3002",
    "mock-specs-backend": "json-server db-specs.json --id projectSlug --watch --port 3003",
    "mock-reports-backend": "json-server db-reports.json --id projectSlug --watch --port 3004",
    "mock-backend": "concurrently \"npm run mock-project-backend\" \"npm run mock-recording-backend\" \"npm run mock-configurations-backend\" \"npm run mock-specs-backend\" \"npm run mock-reports-backend\"",
    "build-backend": "nx build nest-datalayer-checker",
    "frontend": "nx serve frontend",
    "ng-frontend": "nx serve ng-frontend",
    "prod-ng-frontend": "nx serve ng-frontend --prod",
    "build-frontend": "nx build frontend --prod",
    "start-electron": "npm run build-backend && electron .",
    "dev-app": "concurrently \"npm run frontend\" \"npm run backend\"",
    "ng-mock-app": "concurrently \"run mock-backend\" \"npm run ng-frontend\"",
    "build": "npm run build-frontend && npm run build-backend",
    "app": "concurrently \"http-server ./dist/apps/frontend -p 5000 --cors\" \"node ./dist/apps/nest-datalayer-checker/main.js\"",
    "start": "electron-forge start",
    "package": "cross-env NODE_ENV=production && npm run build && electron-forge package",
    "make": "electron-forge make"
  },
  "private": true,
