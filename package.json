{
  "name": "tag-check",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Guan Xin Wang, Brandon Pardede",
  "description": "A tool to check the data layer of a website",
  "license": "MIT",
  "scripts": {
    "backend": "nx serve nest-backend --watch",
    "mock-project-backend": "nodemon mock/mock-server/mock-project-backend.js",
    "mock-recording-backend": "json-server mock/mock-db/db-recordings.json --id projectSlug --watch --port 3001",
    "mock-configurations-backend": "json-server mock/mock-db/db-configurations.json --id name --watch --port 3002",
    "mock-specs-backend": "json-server mock/mock-db/db-specs.json --id projectSlug --watch --port 3003",
    "mock-reports-backend": "nodemon mock/mock-server/mock-reports-backend.js",
    "mock-settings-backend": "nodemon mock/mock-server/mock-settings-backend.js",
    "mock-backend": "concurrently \"npm run mock-project-backend\" \"npm run mock-recording-backend\" \"npm run mock-configurations-backend\" \"npm run mock-specs-backend\" \"npm run mock-reports-backend\" \"npm run mock-settings-backend\"",
    "build-backend": "nx build nest-backend",
    "ng-frontend": "nx serve ng-frontend",
    "ng-tag-build": "nx serve ng-tag-build",
    "prod-ng-frontend": "nx serve ng-frontend --prod",
    "prod-ng-tag-build": "nx serve ng-tag-build --prod",
    "build-ng-frontend": "nx build ng-frontend --configuration production --optimization --base-href ./",
    "bundle-ng-frontend-report": "nx build ng-frontend --configuration production --source-map && source-map-explorer dist/apps/ng-frontend/browser/*.js",
    "build-ng-tag-build": "nx build ng-tag-build --configuration production --optimization",
    "bundle-ng-tag-build-report": "nx build ng-tag-build --configuration production --source-map && source-map-explorer dist/apps/ng-tag-build/browser/*.js",
    "start-ng-electron": "cross-env-shell NODE_ENV=ng-frontend \"npm run build-ng-frontend && electron .\"",
    "ng-dev-app": "concurrently \"npm run ng-frontend\" \"npm run backend\"",
    "ng-mock-app": "concurrently \"run mock-backend\" \"npm run ng-frontend\"",
    "build-ng": "cross-env-shell NODE_ENV=ng-frontend \"npm run build-ng-frontend && npm run build-backend\"",
    "ng-app": "concurrently \"http-server ./dist/apps/ng-frontend/browser -p 5000 --cors\" \"node ./dist/apps/nest-backend/main.js\"",
    "packager-ng": "cross-env-shell NODE_ENV=ng-frontend \"npm run build-ng && electron-forge package\"",
    "start": "electron-forge start",
    "make": "electron-forge make"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.1.0",
    "@angular-devkit/core": "~17.1.0",
    "@angular-devkit/schematics": "~17.1.0",
    "@angular-eslint/eslint-plugin": "~17.0.0",
    "@angular-eslint/eslint-plugin-template": "~17.0.0",
    "@angular-eslint/template-parser": "~17.0.0",
    "@angular/cli": "~17.1.0",
    "@angular/compiler-cli": "~17.1.0",
    "@angular/language-service": "~17.1.0",
    "@angular/material": "~17.1.2",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/angular": "17.2.8",
    "@nx/cypress": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/nest": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/web": "17.2.8",
    "@nx/webpack": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@schematics/angular": "~17.1.0",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.11",
    "@types/node": "18.16.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "cross-env": "^7.0.3",
