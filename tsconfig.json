{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist/apps/electron",
    "moduleResolution": "node",
    "resolveJsonModule": true
    // "paths": {
    //   "*": ["node_modules/*"]
    // }
  },
  "include": ["src/**/*"]
}
